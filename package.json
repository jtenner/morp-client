{
	"name": "morp-client",
	"displayName": "morp-client",
	"description": "An in-ide mud client that is compatible with morp servers for easy game object editing.",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.36.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "morp-client:morp-connect",
				"title": "Connect to Morp"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^12.7.2",
		"@types/vscode": "^1.37.0",
		"glob": "^7.1.4",
		"mocha": "^10.2.0",
		"tslint": "^5.19.0",
		"typescript": "^3.5.3",
		"vscode-test": "^1.2.0"
	},
	"dependencies": {
		"ts-telnet2": "^0.2.7"
	}
}
